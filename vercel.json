{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 60
    },
    "app/api/ai-setup/generate-manual/route.ts": {
      "maxDuration": 300
    },
    "app/api/ai-setup/analyze-media/route.ts": {
      "maxDuration": 60
    },
    "app/api/properties/route.ts": {
      "maxDuration": 60
    },
    "app/api/properties/safe/route.ts": {
      "maxDuration": 60
    },
    "app/api/chatbot/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/guest-followup",
      "schedule": "0 10 * * *"
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}